<Window x:Class="DependencyInjectionExmple.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DependencyInjectionExmple"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical">
            <TextBlock HorizontalAlignment="Center" Text="Dependency Injection Service example" FontSize="20" FontWeight="Bold"/>
            <Button Content="Show dependency Injection" FontSize="25" FontWeight="Bold" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="75,279,0,0" Width="665" Height="65" Command="{Binding ShowDependencyInjectionCommand}"/>
        </StackPanel>
    </Grid>
</Window>
